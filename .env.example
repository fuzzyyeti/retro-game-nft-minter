# Example .env file for pixelplay-minter

# Solana RPC endpoint
RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY

# Path to your Solana wallet keypair (JSON file)
KEY_PATH=/path/to/your/wallet.json

# Irsy node endpoint for file uploads
IRYS_NODE=https://node1.irys.xyz

# Merkle tree public key (optional for mintNfts, required for minting)
MERKLE_TREE_PUBKEY=YOUR_MERKLE_TREE_PUBLIC_KEY

# Collection NFT public key (required for mintNfts)
COLLECTION=YOUR_COLLECTION_NFT_PUBLIC_KEY

